{
  "rules": {
    "by_username": {
      "$key": {
        ".read": "auth.uid === root.child('admin').val()",
        ".write": "auth.uid === root.child('admin').val()"
      },
      ".read": "auth.uid === root.child('admin').val()",
      ".write": "auth.uid === root.child('admin').val()",
      ".indexOn": [
        "rank", "birth_year", "rating", "highest_rating", "competitions", "wins",
        "country", "formal_country_name", "crown", "user_color", "affiliation"
      ]
    },
    "by_twitter_id": {
      "$key": {
        ".read": "auth.uid === root.child('admin').val()",
        ".write": "auth.uid === root.child('admin').val()"
      },
      ".read": "auth.uid === root.child('admin').val()",
      ".write": "auth.uid === root.child('admin').val()",
      ".indexOn": [
        "rank", "birth_year", "rating", "highest_rating", "competitions", "wins",
        "country", "formal_country_name", "crown", "user_color", "affiliation"
      ]
    },
    "continuation": {
      ".read": "auth.uid === root.child('admin').val()",
      ".write": "auth.uid === root.child('admin').val()"
    }
  }
}